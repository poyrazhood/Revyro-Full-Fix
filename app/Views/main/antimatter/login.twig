    <!--============= Header Section Starts Here =============-->
    <header class="header-section header-white-dark">
        <div class="container">
            <div class="header-wrapper">
                <div class="logo">
                    <a href="/">
  {% if site["logo"] %}
                                      <img src="{{ site['logo'] }}" alt="{{ site["site_name"] }}" title="{{ site["site_name"] }}">
                                    {% else %}
                                      {{ site["site_name"] }}
                                    {% endif %}                       </a>
                </div>
                <ul class="menu">
         <li {% if active_menu == "auth" %} class="active" {% endif %}>
          <a href="/">{{ lang['signin.title'] }}</a>
        </li> 
        {% if registerPage %}
        <li {% if active_menu == "signup" %} class="active" {% endif %}>
          <a href="/signup">{{ lang['signup.title'] }}</a>
        </li>
        {% endif %}
                  {% if servicesPage %}
        <li {% if active_menu == "services" %} class="active" {% endif %}>
          <a href="/services">{{ lang['services.title'] }}</a>
        </li>
        {% endif %}
        {% if blogPage %}
        <li {% if active_menu == "blog" %} class="active" {% endif %}>
          <a href="/blog">{{ lang['blog.title'] }}</a>
        </li>
        {% endif %}
        {% if faqPage %}
        <li {% if active_menu == "faq" %} class="active" {% endif %}>
          <a href="/faq">{{ lang['faq.title'] }}</a>
        </li>
        {% endif %}
        {% if termsPage %}
        <li {% if active_menu == "terms" %} class="active" {% endif %}>
          <a href="/terms">{{ lang['terms.title'] }}</a>
        </li>
        {% endif %}
        {% if apiPage %}
        <li {% if active_menu == "api" %} class="active" {% endif %}>
          <a href="/api">{{ lang['api.title'] }}</a>
        </li>
        {% endif %}
        {% for menus in menuall %}
            <li><a href="{{ menus.link }}">{{ menus.name }}</a></li> 
        {% endfor %}
                            
                </ul>
                <div class="header-bar d-lg-none">
                    <span></span>
                    <span></span>
                    <span></span>
                </div>
                <div class="header-right">
                              {% for lang in site['languages'] %}
              {% if lang['active'] %}
                    <select class="select-bar">
                   
                        {% for lang in site['languages'] %}
                <option {% if lang['active'] %} selected {% endif %}><a href="?lang={{ lang['code'] }}">{{ lang['name'] }}</a></li>
              {% endfor %}
                    </select>
                        {% endif %}
            {% endfor %}
               
             
                     
                    {% if registerPage %}
                        <a href="/signup" class="header-button d-none d-sm-inline-block light">{{ lang['signup.title'] }}</a>
                    {% endif %}
                </div>
            </div>
        </div>
